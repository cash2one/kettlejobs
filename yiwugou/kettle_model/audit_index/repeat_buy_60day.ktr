    <sql> select count(buyer_id) num,&apos;repeat_buy_60day&apos; index_name, to_char(sysdate-1, &apos;yyyymmdd&apos;) day_str from (
              select buyer_id as buyer_id
               from ( select o.buyer_id as buyer_id, o.order_id as order_id
                           from T_OM_Order o
                           where o.status in (1, 2, 3) 
                            and o.create_time &gt; to_char(sysdate-60, &apos;yyyymmdd&apos;)||&apos;000000&apos;
                            and o.create_time &lt; to_char(sysdate, &apos;yyyymmdd&apos;)||&apos;000000&apos;
                           group by o.buyer_id, o.order_id)
                            group by buyer_id 

        <class_source><![CDATA[import org.pentaho.di.core.database.*;
import com.yiwugou.analysis.util.ip2district.*;
import java.sql.Timestamp;


String agent;
String op1;
String op2;
String op3;
String op4;
String op5;
String op6;
String op7;
int begin = 0;
int end =50;
//ip,sub_ip,vist_time,yiwugouid,agent,referer,CONCAT(host_name,url) url
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
 //
//logError("begin:");
// ip,sub_ip,vist_time,yiwugouid,agent,referer,CONCAT(host_name,url) url
 Object[] r = getRow();
    // If the row object is null, we are done processing.
 //
 if (r == null||r.length==0) {
  setOutputDone();
  return false;
      // return true;
 }

 // Let's look up parameters only once for performance reason.
//logError("begin1:");
 if (first) {
agent = getParameter("agent");
  op1 = getParameter("op1");
  op2 = getParameter("op2");
  op3 = getParameter("op3");
  op4 = getParameter("op4");
  op5 = getParameter("op5");
  op6 = getParameter("op6");
  op7 = getParameter("op7");

 //logError("referer"+referer);
first =false;

 }


    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
 //logError("begin2:");
 String agentStr = get(Fields.In, agent).getString(r);
//logError("begin4:");
//String address_str = get(Fields.In, address).getString(r);

//logError("ipStr:"+ipStr);
//logError("sub_ip_str:"+sub_ip_str);
//logError("agentStr:"+agentStr);
String [] agents = agentStr.split("[()]");
//logError("agentLength:"+agents.length);
if(agents.length>=2)
    {
	String opValue = agents[1];
//logError("opValue:"+opValue);
	String []opValues = opValue.split(";");


if(opValues.length>=1)
get(Fields.Out,op1).setValue(outputRow,(opValues[0].length()<=end?opValues[0]:opValues[0].substring(begin,end)).trim() );
if(opValues.length>=2)
get(Fields.Out,op2).setValue(outputRow,(opValues[1].length()<=end?opValues[1]:opValues[1].substring(begin,end)).trim() );
if(opValues.length>=3)
get(Fields.Out,op3).setValue(outputRow,(opValues[2].length()<=end?opValues[2]:opValues[2].substring(begin,end)).trim() );
if(opValues.length>=4)
{
get(Fields.Out,op4).setValue(outputRow,(opValues[3].length()<=end?opValues[3]:opValues[3].substring(begin,end)).trim()  );

}
if(opValues.length>=5)
{
get(Fields.Out,op5).setValue(outputRow,(opValues[4].length()<=end?opValues[4]:opValues[4].substring(begin,end)).trim()  );
logError("opValues[4]:"+opValues[4]);
}
if(opValues.length>=6)
get(Fields.Out,op6).setValue(outputRow,(opValues[5].length()<=end?opValues[5]:opValues[5].substring(begin,end)).trim()  );
if(opValues.length>=7)
get(Fields.Out,op7).setValue(outputRow,(opValues[6].length()<=end?opValues[6]:opValues[6].substring(begin,end)).trim()  );

    putRow(data.outputRowMeta, outputRow);


	}



 return true;
}
        ]]></class_source>

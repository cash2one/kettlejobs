  <step>
    <name>domain_subdomain_pv_rate</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>nginx</connection>
    <sql>SELECT *,search_num&#47;visit_num search_rate,shop_num&#47;visit_num shop_rate,product_num&#47;visit_num product_rate,order_num&#47;visit_num order_rate,&apos;${YESTERDAY_STR}&apos; day_date
FROM 
(SELECT a.domain,a.subdomain,COUNT(1) visit_num,IFNULL(search_num,0) search_num,IFNULL(shop_num,0) shop_num,IFNULL(product_num,0) product_num,IFNULL(order_num,0) order_num
FROM fact_sem_seo_oneday_decode AS a
LEFT JOIN
	(	
SELECT COUNT(1) search_num,domain,subdomain
		FROM fact_sem_seo_oneday_decode f1,
		etl_middle_sem_seo_access_detail e1
		WHERE f1.session_id = e1.session_id
		AND e1.visit_url IN (&apos;&#47;search&#47;s.html&apos;,&apos;&#47;search&#47;sshop.html&apos;,&apos;&#47;search&#47;sproduct.html&apos;)
		GROUP BY domain,subdomain
	) AS b
ON a.domain=b.domain AND a.subdomain=b.subdomain 
LEFT JOIN
	(	
SELECT COUNT(1) shop_num,domain,subdomain
		FROM fact_sem_seo_oneday_decode f1,
		etl_middle_sem_seo_access_detail e1
		WHERE f1.session_id = e1.session_id
		 AND e1.visit_url =&apos;&#47;hu&#47;&apos;
		GROUP BY domain,subdomain
	) AS c
ON a.domain=c.domain AND a.subdomain=c.subdomain 
LEFT JOIN
	(SELECT COUNT(1) product_num,domain,subdomain
		FROM fact_sem_seo_oneday_decode f1,
		etl_middle_sem_seo_access_detail e1
		WHERE f1.session_id = e1.session_id
		 AND e1.visit_url=&apos;&#47;product&#47;detail&#47;&apos;
		GROUP BY domain,subdomain
	) AS d
ON a.domain=d.domain AND a.subdomain=d.subdomain 
LEFT JOIN
	(	
		SELECT COUNT(1) order_num,domain,subdomain
		FROM fact_sem_seo_oneday_decode f1,
		etl_middle_sem_seo_access_detail e1
		WHERE f1.session_id = e1.session_id
		 AND e1.visit_url=&apos;&#47;neworder&#47;orderConfirm.html&apos;
		GROUP BY domain,subdomain
	) AS e
ON a.domain=e.domain AND a.subdomain=e.subdomain
where a.domain is not null  
GROUP BY subdomain,domain) AS t
where visit_num&gt;20
order by 3 desc;


</sql>

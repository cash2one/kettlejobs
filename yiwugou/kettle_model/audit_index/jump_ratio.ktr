    <sql>-- 1.1 首页来自外网的访问UV数
  select count(distinct(concat(server_name,yiwugouid,session_id_year,agent))) num, &apos;www_index_fw_visit_uv&apos; index_name,
  DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) day_str
   FROM nginx_log.nginx_access_log_raw
 WHERE vist_time &gt;=   DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) AND  vist_time &lt; DATE_FORMAT(NOW(),&apos;%Y%m%d&apos;) 
 and host_name = &apos;www.yiwugou.com&apos; and visit_url = &apos;&#47;&apos; and visit_referer &lt;&gt; &apos;-&apos; and visit_referer not like &apos;%yiwugou%&apos; 
 and visit_referer not like &apos;%yiwubuy%&apos;
 union
-- 其他页面来自首页的跳转（站外来源）
 select count(1) num, &apos;www_index_fw_next_uv&apos; index_name, DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) day_str 
 from ywg_warehouse.middle_www_index_visit_user t1,ywg_warehouse.middle_www_index_next_user t2 where t1.m_char = t2.m_char
 union 
 -- 1.wap首页的跳出率 235 210
  select count(distinct(concat(server_name,yiwugouid,session_id_year,agent))) num, &apos;wap_index_fw_visit_uv&apos; index_name,
  DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) day_str
   FROM nginx_log.nginx_access_log_raw
 WHERE vist_time &gt;=   DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) AND  vist_time &lt; DATE_FORMAT(NOW(),&apos;%Y%m%d&apos;) 
 and host_name = &apos;wap.yiwugou.com&apos; and visit_url = &apos;&#47;&apos; and visit_referer &lt;&gt; &apos;-&apos; and visit_referer not like &apos;%yiwugou%&apos; 
 and visit_referer not like &apos;%yiwubuy%&apos;
 union 
-- 7083 统计花费时间 4.25秒  来自外网的访问在首页跳到其他页面的uv数
 select count(1) num, &apos;wap_index_fw_next_uv&apos; index_name, DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) day_str 
 from ( select distinct(concat(server_name,yiwugouid,session_id_year,agent)) m_char  FROM nginx_log.nginx_access_log_raw
 WHERE vist_time &gt;=   DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) AND  vist_time &lt; DATE_FORMAT(NOW(),&apos;%Y%m%d&apos;) 
 and host_name = &apos;wap.yiwugou.com&apos; and visit_url = &apos;&#47;&apos; and visit_referer &lt;&gt; &apos;-&apos; and visit_referer not like &apos;%yiwugou%&apos; 
 and visit_referer not like &apos;%yiwubuy%&apos;)t1,(select distinct(concat(server_name,yiwugouid,session_id_year,agent)) m_char1
 FROM nginx_log.nginx_access_log_raw
 WHERE vist_time &gt;=   DATE_FORMAT(DATE_SUB(NOW(),INTERVAL 1 DAY),&apos;%Y%m%d&apos;) AND  vist_time &lt; DATE_FORMAT(NOW(),&apos;%Y%m%d&apos;) 
 and host_name = &apos;wap.yiwugou.com&apos; and visit_referer = &apos;http:&#47;&#47;wap.yiwugou.com&#47;&apos; and visit_url &lt;&gt;  &apos;&#47;&apos;) t2 where t1.m_char = t2.m_char1
 
 </sql>

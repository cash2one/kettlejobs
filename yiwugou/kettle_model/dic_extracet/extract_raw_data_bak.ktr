        <class_source><![CDATA[import org.pentaho.di.core.database.*;
import java.io.UnsupportedEncodingException;
import com.yiwugou.analysis.util.dic.SeperateWordFetcher;



String titleField;
String tagField;
String introductionField;
String useridField ;
String wordField;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
 //  title,tags,introduction,user_id 
 Object[] r = getRow();

    // If the row object is null, we are done processing.
 //
 if (r == null||r.length==0) {
  setOutputDone();
  return false;
      //   return true;
 }


 // Let's look up parameters only once for performance reason.
 //

 if (first) {
	titleField = getParameter("title");
  	tagField = getParameter("tags");
  	introductionField = getParameter("introduction");
    useridField = getParameter("user_id");
	wordField=getParameter("word");
 }
  

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
 //
 
  //  Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
//Object[] outputRow = createOutputRow(r,  data.outputRowMeta.size());

 String title = get(Fields.In, titleField).getString(r);
 String tags = get(Fields.In, tagField).getString(r);
 String introduction = get(Fields.In, introductionField).getString(r);
 String user_id = get(Fields.In, useridField).getString(r);




//logError("title:"+title);


//putRow(data.outputRowMeta, outputRow);
if(introduction!=null&&introduction.trim().length()>0)
{
String [] resultWord = SeperateWordFetcher.getWordOneRow(introduction);

for(int i=0;i<resultWord.length;i++ )
{
String wRow = resultWord[i];
if(wRow==null||wRow.trim().length()==0)
	continue;
wRow = wRow.trim();
if(wRow.length()>400)
 wRow = wRow.substring(0, 400);
Object[] clonedRow = getInputRowMeta().cloneRow(r);
   clonedRow = createOutputRow(clonedRow, clonedRow.length);
get(Fields.Out, wordField).setValue(clonedRow,wRow);
    putRow(data.outputRowMeta, clonedRow);
}

}

if(title!=null&&title.trim().length()>0)
{
String [] titleWord = SeperateWordFetcher.getWordOneRow(title);
for(int i=0;i<titleWord.length;i++ )
{
String wRow = titleWord[i];
if(wRow==null||wRow.trim().length()==0)
	continue;
wRow = wRow.trim();
if(wRow.length()>400)
 wRow = wRow.substring(0, 400);
Object[] clonedRow = getInputRowMeta().cloneRow(r);
   clonedRow = createOutputRow(clonedRow, clonedRow.length);
get(Fields.Out, wordField).setValue(clonedRow,wRow);
    putRow(data.outputRowMeta, clonedRow);
}
}

if(tags!=null&&tags.trim().length()>0)
{
String [] tagWord = SeperateWordFetcher.getWordOneRow(tags);
for(int i=0;i<tagWord.length;i++ )
{
String wRow = tagWord[i];
if(wRow==null||wRow.trim().length()==0)
	continue;
wRow = wRow.trim();
if(wRow.length()>400)
 wRow = wRow.substring(0, 400);
Object[] clonedRow = getInputRowMeta().cloneRow(r);
   clonedRow = createOutputRow(clonedRow, clonedRow.length);
get(Fields.Out, wordField).setValue(clonedRow,wRow);
    putRow(data.outputRowMeta, clonedRow);
}
}

return true;

}]]></class_source>

 t1.userid &quot;USERID&quot; ,nvl(t3.SHOPCOUNT,0) shopcount ,nvl(t2.PRODUCTCOUNT,0) productcount,nvl(t2.PRODUCTCOUNT,0) +nvl(t3.SHOPCOUNT,0) TOTALCOUNT,to_char(sysdate-1, &apos;yyyymmdd&apos;) &quot;TIME&quot;,
 t1.IPCOUNT from 
     (
     select userid,count(distinct(ip)) IPCOUNT from LOG_ANALYSIS_TONGJI where type in(2,3) and time = to_char(sysdate-1, &apos;yyyymmdd&apos;) and source not like &apos;%yiwugou%&apos; and source &lt;&gt; &apos;-&apos;
    group by userid )t1 left join 

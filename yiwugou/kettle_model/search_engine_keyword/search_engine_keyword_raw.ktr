        <class_source><![CDATA[import org.pentaho.di.core.database.*;
import java.io.UnsupportedEncodingException;
import java.net.URLDecoder;
import com.yiwugou.analysis.util.kettle.userdefine.SearchEngineKeyWord;


String num;
String referer;
String key_word;
String dayStr;
String site_source;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
 //
 Object[] r = getRow();

    // If the row object is null, we are done processing.
 //
 if (r == null||r.length==0) {
  setOutputDone();
  return false;
      //   return true;
 }

 // Let's look up parameters only once for performance reason.

 if (first) {
  num = getParameter("num");
  referer = getParameter("referer");
    key_word = getParameter("key_word");
dayStr = getParameter("dayStr");
site_source = getParameter("site_source");
 //logError("url_slim_str12"+url);
first =false;
 }

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
 
 String refererStr = get(Fields.In, referer).getString(r);
 
 // Set the value in the output field
   // logError("url_slim_str234"+urlStr);
 String key_word_str = "";

key_word_str = SearchEngineKeyWord.decode(refererStr);
if(key_word_str==null||key_word_str.trim().length()==0)
	return true;

get(Fields.Out,key_word).setValue(outputRow,key_word_str );
    
    putRow(data.outputRowMeta, outputRow);

 return true;
}]]></class_source>

   (  select userid,count(1) SHOPCOUNT from LOG_ANALYSIS_TONGJI where type =2 and time = to_char(sysdate-1, &apos;yyyymmdd&apos;) and source not like &apos;%yiwugou%&apos; and source &lt;&gt; &apos;-&apos;

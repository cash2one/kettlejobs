    <sql>truncate TABLE middle_access_log_raw;  -- 临时的当天原始表
insert into middle_access_log_raw
(SELECT *
FROM nginx_access_log_raw
WHERE vist_time&gt;=&apos;${YESTERDAY_STR}&apos; AND vist_time&lt;&apos;${DAY_STR}&apos; );

TRUNCATE TABLE middle_yiwugouid_buyer_id;  -- 采购商id
INSERT INTO middle_yiwugouid_buyer_id
(SELECT DISTINCT yiwugouid  buyer_id 
FROM middle_access_log_raw
WHERE yiwugouid&lt;&gt;&apos;-&apos;
AND url=&apos;&#47;order&#47;buy&#47;index&#47;1.htm?payStatus=all&amp;pageSize=20&apos;);

TRUNCATE TABLE middle_yiwugouid_seller_id;  -- 供应商id
INSERT INTO middle_yiwugouid_seller_id
(SELECT DISTINCT yiwugouid  seller_id 
FROM middle_access_log_raw
WHERE yiwugouid&lt;&gt;&apos;-&apos;
AND url=&apos;&#47;order&#47;sell&#47;index&#47;1.htm?pageSize=10&apos;);

insert into buyer_seller_session_id_num
(SELECT buyer_session_num,buyer_id_num,seller_session_num,seller_id_num,&apos;${YESTERDAY_STR}&apos; day_date
FROM  
	(-- 采购商session_id_year及yiwugouid数
	SELECT  COUNT(DISTINCT session_id_year) buyer_session_num,COUNT(DISTINCT yiwugouid) buyer_id_num
	FROM middle_access_log_raw n,middle_yiwugouid_buyer_id m
	WHERE n.yiwugouid=m.buyer_id AND vist_time&gt;=&apos;${YESTERDAY_STR}&apos; AND vist_time&lt;&apos;${DAY_STR}&apos; )AS a,	
	(-- 供应商商session_id_year及yiwugouid数
	SELECT  COUNT(DISTINCT session_id_year) seller_session_num,COUNT(DISTINCT yiwugouid) seller_id_num
	FROM middle_access_log_raw n,middle_yiwugouid_seller_id m
	WHERE n.yiwugouid=m.seller_id AND vist_time&gt;=&apos;${YESTERDAY_STR}&apos; AND vist_time&lt;&apos;${DAY_STR}&apos; ) AS b
);</sql>

        <class_source><![CDATA[import org.pentaho.di.core.database.*;
import com.yiwugou.analysis.util.ip2district.*;
import java.sql.Timestamp;

String ip;
String sub_ip;
Timestamp vist_time;
String yiwugouid;
String agent;
String referer;
String url;
String address;
//ip,sub_ip,vist_time,yiwugouid,agent,referer,CONCAT(host_name,url) url
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
 //
//logError("begin:");
// ip,sub_ip,vist_time,yiwugouid,agent,referer,CONCAT(host_name,url) url
 Object[] r = getRow();
    // If the row object is null, we are done processing.
 //
 if (r == null||r.length==0) {
  setOutputDone();
  return false;
      // return true;
 }

 // Let's look up parameters only once for performance reason.
//logError("begin1:");
 if (first) {
ip = getParameter("ip");
  sub_ip = getParameter("sub_ip");
address = getParameter("address");

 //logError("referer"+referer);
first =false;

 }


    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
 //logError("begin2:");
 String ipStr = get(Fields.In, ip).getString(r);
//logError("begin3:");
String sub_ip_str = get(Fields.In, sub_ip).getString(r);
//logError("refererStr:"+refererStr);
//logError("begin4:");
//String address_str = get(Fields.In, address).getString(r);

//logError("ipStr:"+ipStr);
//logError("sub_ip_str:"+sub_ip_str);


 
IPSeeker seeker = IPSeeker.getInstance();
		String addressStr = seeker.getAddress(ipStr,sub_ip_str);

//logError("addressStr:"+addressStr);

if(addressStr!=null&&addressStr.trim().length()>=50)
  addressStr = addressStr.substring(50);

get(Fields.Out,address).setValue(outputRow,addressStr );
    putRow(data.outputRowMeta, outputRow);

 return true;
}

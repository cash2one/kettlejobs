        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import org.pentaho.di.core.database.*;
import java.io.UnsupportedEncodingException;
import java.net.URLDecoder;
import com.yiwugou.analysis.util.kettle.userdefine.SearchEngineKeyWord;

String referer;
String key_word;
String domain;
String subdomain;
String sourcetype;
String url;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
 //
 Object[] r = getRow();
    // If the row object is null, we are done processing.
 //
 if (r == null||r.length==0) {
  setOutputDone();
  return false;
      // return true;
 }

 // Let's look up parameters only once for performance reason.

 if (first) {
referer = getParameter("referer");
  url = getParameter("url");
  key_word = getParameter("key_word");
domain = getParameter("domain");
subdomain = getParameter("subdomain");
sourcetype  = getParameter("sourceType");
 //logError("referer"+referer);
first =false;
 }

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
 
 String refererStr = get(Fields.In, referer).getString(r);
String urlStr = get(Fields.In, url).getString(r);
//logError("refererStr:"+refererStr);
 
 // Set the value in the output field
   // logError("url_slim_str234"+urlStr);
 String key_word_str = "";


		String source_type = "seo";
		if(urlStr.toUpperCase().contains("EQN=S"))
			source_type = "sem";
		
		refererStr = refererStr.replaceFirst("http*.://", "");
		String [] domainArrs = refererStr.split("/");
		String domainStr = "";
		if(domainArrs.length>=1)
			domainStr = domainArrs[0];
		//System.out.println("domain:"+domain);
		
		String []domainCompo = domainStr.split("\\.");
		String uppestDomain = domainStr;
		if(domainCompo.length>=3){
			if(domainStr.indexOf(".")>0)
			uppestDomain = domainStr.substring(domainStr.indexOf(".")+1);//domainCompo[domainCompo.length-2]+"."+domainCompo[domainCompo.length-1];
		}
		



key_word_str = SearchEngineKeyWord.decode(refererStr);



if(key_word_str!=null&&key_word_str.trim().length()>=50)
  key_word_str = key_word_str.substring(50);

get(Fields.Out,key_word).setValue(outputRow,key_word_str );

get(Fields.Out,domain).setValue(outputRow, uppestDomain);

get(Fields.Out,subdomain).setValue(outputRow, domainStr );

get(Fields.Out,sourcetype).setValue(outputRow, source_type );

   
    putRow(data.outputRowMeta, outputRow);

 return true;
}]]></class_source>

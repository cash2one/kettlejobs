    <sql>

-- 创建入口页的session_id排重集合
truncate table etl_middle_sem_seo_sid_unique;
insert into etl_middle_sem_seo_sid_unique (session_id) 
	 SELECT DISTINCT(session_id) FROM nginx_access_log_raw 
 where vist_time &gt;=&apos;${YESTERDAY_STR}&apos; AND  vist_time &lt;&apos;${DAY_STR}&apos; and visit_referer NOT LIKE &apos;%yiwugou.com%&apos; AND visit_referer NOT LIKE &apos;%onccc.com%&apos; AND visit_referer NOT LIKE &apos;%yiwubuy.com%&apos; AND visit_referer&lt;&gt;&apos;-&apos;
 AND session_id &lt;&gt; &apos;-&apos;;

-- 创建入口页的集合，包括session_id ,url , vist_time,可用于后续删除
truncate table etl_middle_sem_seo_enter_detail;
insert into etl_middle_sem_seo_enter_detail
 (SELECT referer , vist_time,visit_url ,session_id ,visit_referer,url FROM nginx_access_log_raw 
WHERE vist_time &gt;=&apos;${YESTERDAY_STR}&apos; AND  vist_time &lt;&apos;${DAY_STR}&apos; and visit_referer NOT LIKE &apos;%yiwugou.com%&apos; AND visit_referer NOT LIKE &apos;%onccc.com%&apos; AND visit_referer NOT LIKE &apos;%yiwubuy.com%&apos; AND visit_referer&lt;&gt;&apos;-&apos;);

-- AND session_id &lt;&gt; &apos;-&apos;
-- 所有跟seesion_id能关联上的记录
truncate table etl_middle_sem_seo_access_detail;
insert into  etl_middle_sem_seo_access_detail
 (SELECT n.session_id,yiwugouid,n.vist_time,n.url,n.visit_url,n.agent
  FROM nginx_access_log_raw n ,etl_middle_sem_seo_sid_unique e 
  where vist_time &gt;=&apos;${YESTERDAY_STR}&apos; AND  vist_time &lt;&apos;${DAY_STR}&apos; and n.session_id = e.session_id);

-- 删除掉入口页的记录,关联删除
   DELETE a FROM etl_middle_sem_seo_access_detail AS a JOIN 
   etl_middle_sem_seo_enter_detail AS b ON a.session_id = b.session_id  AND  a.url = b.url and a.vist_time=b.vist_time</sql>
